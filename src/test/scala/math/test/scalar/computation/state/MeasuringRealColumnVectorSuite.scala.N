package math.test.scalar.executable.state

import scala.util.{Random}

import math.examples.scalar.executable.state.{
  measuredNormedRealColumnVector,
  twiceMeasuredNormedRealColumnVector
}

class MeasuringRealColumnVectorSuite extends munit.FunSuite:

  given Random = Random(123L)

  test(
    "measuredNormedRealColumnVector == twiceMeasuredNormedRealColumnVector"
  ):
    assert(
      measuredNormedRealColumnVector ==
        twiceMeasuredNormedRealColumnVector,
      true
    )
